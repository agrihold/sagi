<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data>
 
    <record model="ir.property" id="sgr.sgr_contract_property_general_account_id">
        <field name="name">sgr_contract_property_general_account_id</field>
        <field name="fields_id" search="[('model','=','sgr.contract'), ('name','=','property_general_account_id')]"/>
        <field name="value" eval="'account.account,%i' % ref('account_root')"/>
	</record>
    <record model="ir.property" id="sgr.sgr_contract_property_analytic_account_root_id">
        <field name="name">sgr_contract_property_analytic_account_root_id</field>
        <field name="fields_id" search="[('model','=','sgr.contract'), ('name','=','property_analytic_account_root_id')]"/>
        <field name="value" eval="'account.analytic.account,%i' %  ref('analytic_root')"/>
	</record>
    <record model="ir.property" id="sgr.sgr_contract_property_journal_id">
        <field name="name">sgr_contract_property_journal_id</field>
        <field name="fields_id" search="[('model','=','sgr.contract'), ('name','=','property_journal_id')]"/>
        <field name="value" eval="'account.analytic.journal,%i' % ref('analytic_journal')"/>
	</record>


</data>
</openerp>
        
