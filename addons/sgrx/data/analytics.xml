<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data noupdate="True">
        <record model='account.journal' id='account_journal'>
            <field name="name">Laboratory Contract Journal</field>
            <field name="code">LC</field>
            <field name="type">general</field>
        </record>
        <record model="account.account.type" id="account_type">
          <field name="name">Laboratory Contract</field>
          <field name="code">view</field>
          <field name="close_method">none</field>
        </record>
        <record model='account.account' id='account_root'>
            <field name="name">Laboratory Contract Account</field>
            <field name="code">LC</field>
            <field name="type">view</field>
            <field name="user_type" ref="account_type"/>
        </record>
        <record model='account.analytic.journal' id='analytic_journal'>
            <field name="name">Laboratory Contract Analytic Journal</field>
            <field name="code">LC</field>
            <field name="type">general</field>
        </record>
        <record model='account.analytic.account' id='analytic_root'>
            <field name="name">Laboratory Contracts Analytic Account</field>
            <field name="code">LC</field>
            <field name="type">view</field>
            <field name="state">open</field>
        </record>
    </data>
</openerp>
      
